<p id="notice"><%= notice %></p>

<h1>Game Sessions</h1>

<table id="example" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">
  <thead>
    <tr>
      <th>Period</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @game_sessions.each do |game_session| %>
      <tr>
        <td><%= game_session.period %></td>
        <td><%= link_to 'Show', game_session %></td>
        <td><%= link_to 'Edit', edit_game_session_path(game_session) %></td>
        <td><%= link_to 'Destroy', game_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game Session', new_game_session_path, class: 'btn btn-info' %>
